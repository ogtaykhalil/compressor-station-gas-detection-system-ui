	<Style x:Key="st1" TargetType="Button">
	    <Setter Property="Foreground" Value="#FFFEFEFE" />
	    <Setter Property="Height" Value="50" />

	    <Setter Property="Width" Value="50" />

	    <Setter Property="Margin" Value="0" />
	    <Setter Property="Template">
		<Setter.Value>
		    <ControlTemplate TargetType="Button">
			<Border Name="border"  BorderThickness="0,0,0,0"  BorderBrush="#FF00375D"  Opacity="0.75">
			    <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" />
			    <Border.Background>
				<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
				    <GradientStop Color="#FF2E7D32" Offset="0.532"/>

				    <GradientStop Color="#FF1B5E20" Offset="1"/>
				    <GradientStop Color="#FF1B5E20" Offset="0"/>
				</LinearGradientBrush>
			    </Border.Background>
			</Border>
			<ControlTemplate.Triggers>
			    <Trigger Property="IsMouseOver" Value="True">
				<!--<Setter Property="Effect">
							 <Setter.Value>
								<DropShadowEffect Color="White" ShadowDepth="0" Direction="270" Opacity="1" BlurRadius="8" RenderingBias="Performance"/>
							 </Setter.Value>
						  </Setter>-->
				<Setter TargetName="border" Property="Background" >
				    <Setter.Value>

					<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
					    <GradientStop Color="#FF00759D" Offset="0.532"/>

					    <GradientStop Color="#FF00305D" Offset="1"/>
					    <GradientStop Color="#FF00305D" Offset="0"/>
					</LinearGradientBrush>

				    </Setter.Value>
				</Setter>
				<Setter TargetName="border" Property="Button.Foreground" Value="White" />
				<Setter TargetName="border" Property="BorderThickness" Value="0,0,0,0" />
				<Setter TargetName="border" Property="BorderBrush" Value="#FF025287" />
				<Setter TargetName="border" Property="Opacity" Value="0.8"/>
			    </Trigger>
			    <Trigger Property="IsFocused" Value="True">


				<Setter TargetName="border" Property="Background" >
				    <Setter.Value>

					<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
					    <GradientStop Color="#FF00759D" Offset="0.532"/>

					    <GradientStop Color="#FF00305D" Offset="1"/>
					    <GradientStop Color="#FF00305D" Offset="0"/>
					</LinearGradientBrush>

				    </Setter.Value>
				</Setter>
				<Setter TargetName="border" Property="BorderBrush" Value="#FF007ACC"/>
				<Setter TargetName="border" Property="Button.Foreground" Value="White" />
				<Setter TargetName="border" Property="BorderThickness" Value="0,0,0,0" />
				<Setter TargetName="border" Property="Opacity" Value="0.9"/>
			    </Trigger>
			</ControlTemplate.Triggers>
		    </ControlTemplate>
		</Setter.Value>
	    </Setter>
	</Style>

