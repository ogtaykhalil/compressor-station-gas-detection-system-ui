			    <Setter Property="Visibility" Value="Collapsed" />
			    <Style.Triggers>
				<Trigger Property="IsMouseOver" Value="false">
				    <Setter Property="Visibility" Value="Collapsed" />
				</Trigger>
				<DataTrigger Binding="{Binding ElementName=man, Path=IsMouseOver }" Value="true">
				    <Setter Property="Visibility" Value="Visible"/>
