						<Setter Property="Background" Value="#FF766A23"/>
					    </DataTrigger>
					    <DataTrigger Binding="{Binding AllExhaustFanAutoManStatus}" Value="False">
						<Setter Property="Label.Content" Value="AUTO"/>
						<Setter Property="Background" Value="#FF066B06"/>
